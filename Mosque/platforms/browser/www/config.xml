<?xml version='1.0' encoding='utf-8'?>
<widget id="com.Abutimur.mosque" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Mosque</name>
    <description>
        Mosque App.
    </description>
    <author email="info@mosque.abutimur.com" href="http://mosque.abutimur.com">
        Mosque AbuTimur Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="loadUrlTimeoutValue" value="700000" />
    <preference name="iosPersistentFileLocation" value="Compatibility" />
</widget>
